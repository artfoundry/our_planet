<% possible_friends = [] %>
<% current_member = @members.find(params[:id]) %>
<% @members.each do |member| %>
  <% unless member.id == current_member.id || current_member.is_friend?(member) %> 
    <% possible_friends << member%>
  <% end %>
<% end %>


<% possible_friends.each do |friend| %>
<%= friend.first_name %><br>
<% end %>
